<body>
   {{>nav}}
    <div class="contact-container">
        <img src="/static/img/male-call-centre-operator-vector-3413835.jpg" alt="" class="supimg">
        <div class="right-col">
           

            <h1>Contact us</h1>
            
            <form id="contact-form" method="post">
                <label class="supform" for="name">Full name</label>
                <input class="supform" type="text" id="name" name="name" placeholder="Your Full Name" required>
                <label class="supform" for="email">DEPARTMENT</label>
                <input class="supform" type="email" id="email" name="email" placeholder="Your Department" required>
                <label class="supform" for="email">COURSE</label>
                <input class="supform" type="email" id="email" name="email" placeholder="Your Course" required>
                <label class="supform" for="message">QUERY</label>
                <textarea class="supform" rows="6" placeholder="Your query" id="message" name="message" required></textarea>
                <button type="submit" id="submit"
                    name="submit">Send</button>

            </form>
        
        </div>
    </div>

{{>footer}}
</body>

</html>