                <img src="/static/img/899048ab0cc455154006fdb9676964b3.jpg" alt="" class="icon">
                <h1>{{users.name}}</h1>
                <p>SHARDA UNIVERSITY</p>

            </div>
                
            
            
                <form class="supprtform">
                    <div class="inputfield">
                        <label for="name" class="centre">Name :</label>
                       <h1>{{users.name}}</h1> <br>
                    </div>
                   
                    <div class="inputfield">
                        <label for="dname">DEPARTMENT:</label>
                        <h1>{{users.department}}</h1><br>
                    </div>
                   
                  <div class="inputfield">
                    <label for="course">COURSE :</label>
                    <h1>{{users.course}}</h1><br>
                  </div>
                   
                 
                    <div class="inputfield">
                        <label for="section">SECTION :</label>
                        <h1>{{users.section}}</h1><br>
                    </div>
                    <div class="inputfield">
                        <label for="section">EMAIL :</label>
                        <h1>{{users.shardaemail}}</h1><br>
                    </div>
                    <div class="inputfield">
                        <label for="section">PHONE :</label>
                        <h1>{{users.phone}}</h1><br>
                    </div>
                   
                    
                    
    
                </form>
           
        </div>
