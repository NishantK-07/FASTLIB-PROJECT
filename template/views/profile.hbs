                <label class="supform" for="isbn">ISBN NO</label><br>
                <input class="supform" type="text" id="isbn" name="isbn" placeholder="Your Department" required><br>
                
                <button type="submit" id="submit"
                    name="submit">ISSUE NOW</button>

